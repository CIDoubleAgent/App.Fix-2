<div class="container"></div>
    <div class="row">   
        <div class="col s12 m6 offset-m3">
            {{#each posts as |post|}}
                <div class="card blue-grey darken-3">
                    <div class="card-content white-text">
                        <p class="center-align card-content white-text">{{ this.postTitle }}</p>
                        <p class="center-align card-content white-text">{{ this.postContents }}</p>
                    </div>
                </div>
            {{/each}}
        </div>
    </div>
</div>